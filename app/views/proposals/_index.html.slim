.row
  .col-md-12
    .hpanel
      .panel-heading
        h3 Project Proposals
      .panel-body
        table.table.table-bordered.table-striped
          thead
            tr
              th Proposal
              th Project Name
              th Project Status
              th Accepted?
              th Proposal doc
              th

          tbody
            - @proposals.each do |proposal|
              == render 'proposals/proposal', proposal: proposal

        br

